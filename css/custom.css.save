.navbar-toggle {
  min-height: 46px;
}

a {
    color: #003a75;
    text-decoration: none;
}

.navbar-brand {
  position: fixed;
  padding-top: 9px;
}

@media (max-width:390px) {
  .navbar-brand {
    transform: scale(0.8,0.8);
    transform-origin: left center;
  }
}

@media (max-width:330px) {
  .navbar-brand > span {
    display: none;
  }
}

@media (max-width:220px) {
  .navbar-brand {
    display: none;
  }
}

.navbar-brand > img {
  float: left;
  height: 45px;
  margin-right: 5px;
}

.navbar-brand > span {
  color: #003a75;
  float: right;
  font-size: 21px;
  font-family: Arial, sans-serif;
  line-height: 22px;
  font-weight: bold;
  font-style: italic;
  text-shadow:
    -0.3px -0.3px 0 #5d97cc,  
    0.3px -0.3px 0 #5d97cc,
    -0.3px 0.3px 0 #5d97cc,
    0.3px 0.3px 0 #5d97cc;
}

.navbar-nav > li > a {
  padding: 21px 15px;
}

@media (max-width:768px) {
  .navbar-nav > li > a {
    padding: 5px 15px;
  }
}

@media (min-width:768px) and (max-width:991px) {
  .navbar-nav > li > a {
    padding: 21px 5px;
  }
}

/* Set the fixed height of the footer here */
#footer {
  position: relative;
  border-width: 1px 0 0 0;
  border-style: solid;
  border-color: #e7e7e7;
  background-color: #f8f8f8;
  min-height: 72px;
}

.panel-primary>.panel-heading {
  background-color: #003a75;
  border-color: #003a75;
}
.panel-primary{
  border-color: #003a75;
}

#wrap {
  position: relative;
  min-height: 100%;
  min-height: calc(100% - 62px);
}

#wrap > .container {
  padding: 80px 15px 0;
}

#footer img {
  position: absolute;
  left: 50%;
  margin-left: -231px;
  margin-top: 15px;
}

@media (max-width:530px) {
  #footer img {
    transform: scale(0.8,0.8);
    transform-origin: center center;
  }
}

code {
  font-size: 80%;
}

.table-responsive > .table > tbody > tr > td {
  vertical-align: middle;
  white-space: nowrap;
}
.time-column {
  padding-top: 0 !important;
}

.table-responsive > .table.leaders-table {
  cursor: default;
}

.table-responsive > .table.leaders-table > tbody:nth-of-type(odd) {
  background-color: #F9F9F9;
}

.table-responsive > .table.leaders-table > tbody:hover {
  background-color: #F5F5F5;
}

.table-responsive > .table.leaders-table > tbody > tr > td {
  border: none;
}

.table-responsive > .table.leaders-table > tbody + tbody {
  border-top-width: 1px;
}

@media (min-width:550px) and (max-width:768px) {
  .table-responsive > .table.leaders-table {
    font-size: 0.85em;
  }
  .table-responsive > .table.leaders-table > tbody > tr > td {
    padding: 4px;
  }
}

@media (max-width:550px) {
  .table-responsive > .table.leaders-table {
    font-size: 0.7em;
  }
  .table-responsive > .table.leaders-table > tbody > tr > td {
    padding: 2px;
  }
}

.progress-time {
  padding-bottom: 0 !important;
}

.progress-time > .progress {
  margin: 0 0 3px 0;
  height: 16px;
}

button.list-group-item {
  outline-color: transparent;
}

